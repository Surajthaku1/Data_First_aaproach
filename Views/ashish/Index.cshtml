@model IEnumerable<Data_First_aaproach.Employee>
    <table class="table table-bordered">
        <tr>
            <th>id </th>
            <th>name </th>
            <th>Email </th>
            <th>gender </th>
            <th>mobile </th>
        </tr>
        @foreach(var pr in Model)
            {
    <tr>
        <td>@pr.id</td>
        <td>@pr.name</td>
        <td>@pr.email</td>
        <td>@pr.gender</td>
        <td>@pr.mobile</td>
    </tr>

            }

    </table>